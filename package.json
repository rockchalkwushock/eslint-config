{
  "name": "@rockchalkwushock/eslint-config",
  "version": "1.0.7",
  "description": "My eslint config for working with React & TypeScript.",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "email": "me@codybrunner.com",
    "name": "Cody Brunner",
    "url": "https://codybrunner.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rockchalkwushock/eslint-config.git"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.1"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-react",
    "react eslint",
    "prettier",
    "typescript"
  ],
  "scripts": {
    "format": "prettier --write",
    "lint": "eslint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "~5.26.0",
    "@typescript-eslint/parser": "~5.26.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jest": "~26.2.2",
    "eslint-plugin-jsx-a11y": "~6.5.1",
    "eslint-plugin-react": "~7.30.0",
    "eslint-plugin-react-hooks": "~4.5.0",
    "eslint-plugin-testing-library": "~5.5.0"
  },
  "peerDependencies": {
    "eslint": ">=8",
    "prettier": ">=2.6",
    "typescript": ">=4.7"
  },
  "devDependencies": {
    "@arkweid/lefthook": "~0.7.7",
    "@types/node": "~17.0.35",
    "@types/react": "~18.0.9",
    "eslint": "~8.16.0",
    "prettier": "~2.6.2",
    "react": "~18.1.0",
    "typescript": "~4.7.2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
