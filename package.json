{
  "name": "@rockchalkwushock/eslint-config",
  "version": "1.0.3",
  "description": "My eslint config for working with React & TypeScript.",
  "main": "index.js",
  "license": "MIT",
  "author": {
    "email": "me@codybrunner.com",
    "name": "Cody Brunner",
    "url": "https://codybrunner.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rockchalkwushock/eslint-config.git"
  },
  "engines": {
    "node": ">= 14.x"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-react",
    "react eslint",
    "prettier",
    "typescript"
  ],
  "scripts": {
    "commit": "git-cz",
    "format": "prettier --write",
    "lint": "eslint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.3"
  },
  "peerDependencies": {
    "eslint": "^7.14.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.3",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "typescript": "^4.5.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
